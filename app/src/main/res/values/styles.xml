<resources xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.Material3.DayNight">
        <!-- Primary brand color. -->
        <item name="android:windowBackground">@color/bgColor</item>
        <item name="colorPrimary">@color/primaryColor</item>
        <item name="colorPrimaryVariant">@color/primaryDarkColor</item>
        <item name="colorOnPrimary">@color/bgColor</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/secondaryColor</item>
        <item name="colorSecondaryVariant">@color/secondaryDarkColor</item>
        <item name="colorOnSecondary">@color/secondaryTextColor</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/primaryColor</item>
        <!-- Customize your theme here. -->
        <item name="toolbarStyle">@style/Widget.App.Toolbar</item>
        <item name="actionBarStyle">@style/Base.Widget.AppCompat.ActionBar</item>
        <item name="windowActionModeOverlay">true</item>
        <item name="actionModeBackground">@color/primaryColor</item>
        <item name="actionBarTheme">@style/ThemeOverlay.Material3.Dark.ActionBar</item>
        <item name="popupMenuBackground">@color/primaryColor</item>
        <item name="tintColor">@color/tintColor</item>
        <item name="android:textColor">@color/secondaryTextColor</item>
        <item name="android:itemBackground">@color/bgColor</item>
    </style>

    <style name="AlertDialogTheme" parent="ThemeOverlay.MaterialComponents.MaterialAlertDialog">
        <item name="colorSurface">@color/bgColor</item>
    </style>


    <style name="Widget.App.Toolbar" parent="Widget.Material3.Toolbar">
        <item name="android:background">@color/primaryColor</item>
        <item name="android:textColor">@color/primaryTextColor</item>
        <item name="materialThemeOverlay">@style/ThemeOverlay.App.Toolbar</item>
        <item name="actionBarTheme">@style/ThemeOverlay.Material3.Dark.ActionBar</item>
        <item name="android:textColorPrimary">@android:color/white</item>
        <item name="android:textColorSecondary">@android:color/white</item>
        <item name="android:itemBackground">@color/bgColor</item>
    </style>

    <style name="ThemeOverlay.App.Toolbar" parent="">
        <item name="colorSurface">@color/bgColor</item>
        <item name="colorOnSurface">@color/primaryDarkColor</item>
    </style>

    <style name="CarAppTheme" parent="Theme.Material3.Dark.NoActionBar">
        <item name="android:textAppearance" tools:ignore="PrivateResource">
            @style/CarBody1
        </item>
        <item name="carButtonStyle" tools:ignore="PrivateResource">
            @style/CarButton
        </item>
        <item name="carCardStyle" tools:ignore="PrivateResource">
            @style/CarFloatingCard
        </item>
        <item name="carTextViewStyle" tools:ignore="PrivateResource">
            @style/CarTextView
        </item>
    </style>

    <style name="AppTheme.Car" parent="@style/CarAppTheme" tools:ignore="PrivateResource">
        <item name="themedCarBackground">@drawable/background_incar_black</item>
        <item name="selectableItemBackgroundBorderless">@drawable/custom_focus_outline</item>
        <item name="selectableItemBackground">@drawable/custom_focus_outline</item>
    </style>

    <style name="AppTheme.Car.Speedometer">
        <item name="android:padding">20dp</item>

        <item name="sv_backgroundCircleColor">@android:color/transparent</item>
        <item name="sv_centerCircleColor">@android:color/transparent</item>

        <!-- Marks on the dial background are done on the png itself, not rendered dynamiccaly. -->
        <item name="sv_markColor">@android:color/transparent</item>

        <item name="sv_speedTextColor">@color/car_primary</item>

        <item name="sv_speedTextPadding">50dp</item>
        <item name="sv_speedTextPosition">CENTER</item>
        <item name="sv_speedTextSize">32sp</item>
        <item name="sv_textColor">@android:color/transparent</item>
        <item name="sv_textSize">12sp</item>
        <item name="sv_unit" />
        <item name="sv_unitSpeedInterval">4dp</item>
        <item name="sv_unitTextColor">@color/car_primary</item>
        <item name="sv_unitTextSize">20sp</item>
        <item name="sv_unitUnderSpeedText">true</item>
        <item name="sv_withTremble">false</item>
    </style>

